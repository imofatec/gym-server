{
  "name": "gym",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "node --env-file .env --experimental-strip-types --no-warnings --watch ./src/index.ts",
    "db:push": "npx drizzle-kit push --config ./src/main/config/db/drizzle/drizzle.config.ts",
    "db:generate": "npx drizzle-kit generate --config ./src/main/config/db/drizzle/drizzle.config.ts",
    "db:migrate": "npx drizzle-kit migrate --config ./src/main/config/db/drizzle/drizzle.config.ts",
    "db:ui": "npx drizzle-kit studio --config ./src/main/config/db/drizzle/drizzle.config.ts",
    "db:seed": "node --env-file .env --experimental-strip-types --no-warnings --watch ./src/main/config/db/drizzle/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@fastify/jwt": "^9.1.0",
    "@types/node": "^24.0.13",
    "@types/pg": "^8.15.4",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "typescript": "^5.8.3",
    "ultracite": "5.0.35"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "bcryptjs": "^3.0.2",
    "drizzle-orm": "^0.44.3",
    "fastify": "^5.4.0",
    "fastify-type-provider-zod": "^5.0.2",
    "pg": "^8.16.3",
    "zod": "^4.0.5"
  }
}